%tbody
  %br
  %div{:class=>"card-columns"}
    - @partners.sort.each do |partner|
      %div{:class=>"card"}
        -# %h5{:class=>"card-header"}= link_to partner.name, [:admin, partner]
        %div{:class=>"card-img-top"}
          = image_tag(partner.logo, :class=>"card-img-top") if partner.logo.attached?
        %div{:class=>"card-body"}
          %h5{:class=>"card-title"}= link_to partner.name, [:admin, partner]
          %p{:class=>"card-text"}
            = link_to 'Edit', edit_admin_partner_path(partner), class: 'btn btn-sm btn-outline-primary'